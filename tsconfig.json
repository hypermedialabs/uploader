{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "module": "es6",
    "strict": false,
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "noImplicitThis": false,
  },
  "include": ["src/**/*", "declarations.d.ts"],
  "exclude": ["node_modules"],
}
